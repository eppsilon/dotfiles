  "editor.minimap.maxColumn": 80,
  "editor.lineNumbers": "on",
  "editor.renderLineHighlight": "all",
  "editor.codeLens": false,
  "editor.stablePeek": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.revealIfOpen": true,
  "files.insertFinalNewline": true,
  "explorer.openEditors.visible": 15,
  "debug.inlineValues": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "cSpell.showStatus": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.colorDecorators": false,
  "material-icon-theme.showReloadMessage": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.minimap.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.menus": {
    "editor": {
      "blame": true,
      "clipboard": true,
      "compare": true,
      "details": true,
      "history": true,
      "remote": true
    },
    "editorGroup": {
      "blame": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "editorTab": {
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "compare": true,
      "history": true,
      "remote": true
    }
  },
  "gitlens.codeLens.scopes": ["document", "containers", "blocks"],
  "gitlens.heatmap.toggleMode": "window",
  "breadcrumbs.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.tabCompletion": "on",
  "window.restoreWindows": "all",
  "terminal.external.osxExec": "iTerm.app",
  "editor.suggestSelection": "first",
  "typescript.tsserver.maxTsServerMemory": 16384,
  "git.autofetch": true,
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "debug.console.fontFamily": "Fira Code",
  "editor.tokenColorCustomizations": {
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": "variable.key.table",
          "settings": {
            "foreground": "#ff79c6"
          }
        },
        {
          "scope": "variable.key.array",
          "settings": {
            "foreground": "#ff79c6"
          }
        },
        {
          "scope": "constant.other.time",
          "settings": {
            "foreground": "#DCDCAA"
          }
        }
      ]
    }
  },
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "csharp.suppressHiddenDiagnostics": false,
  // "omnisharp.path": "1.35.3",
  "omnisharp.useGlobalMono": "always",
  "debug.onTaskErrors": "abort",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.format.semicolons": "insert",
  "extensions.ignoreRecommendations": false,
  "editor.suggest.showStatusBar": true,
  "explorer.openEditors.sortOrder": "alphabetical",
  "workbench.editor.decorations.colors": true,
  "workbench.editor.decorations.badges": true,
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "highlight.regexes": {
    "(// ?)(TODO:?)([\t ]*[^\n]+(?:(?:\n)(\\s*//[^\n]*)*)*)": [
      {
        "overviewRulerColor": "#ffcc00",
        "backgroundColor": "#d9ad00",
        "color": "#1f1f1f"
      },
      {
        "backgroundColor": "#ffcc00",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d9ad00",
        "color": "#1f1f1f"
      },
      {
        "backgroundColor": "#d9ad00",
        "color": "#1f1f1f"
      }
    ],
    "(// ?)(HACK:?)([\t ]*[^\n]+(?:(?:\n)(\\s*//[^\n]*)*)*)": [
      {
        "overviewRulerColor": "#ff0000",
        "backgroundColor": "#d90000",
        "color": "#1f1f1f"
      },
      {
        "backgroundColor": "#ff0000",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d90000",
        "color": "#1f1f1f"
      }
    ]
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "angular.experimental-ivy": true,
  "editor.formatOnSave": true,
  "[handlebars]": {
    "editor.defaultFormatter": "none",
    "editor.suggest.insertMode": "replace"
  },
  "workbench.editor.untitled.hint": "hidden",
  "editor.fontSize": 13,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "html.format.enable": false,
  "commentWrap.printWidth": 120,
  "vscode-edge-devtools.mirrorEdits": true,
  "editor.codeLensFontFamily": "Fira Code",
  "debug.console.fontSize": 13,
  "editor.renderWhitespace": "all",
  "redhat.telemetry.enabled": false,
  "editor.copyWithSyntaxHighlighting": false,
  "explorer.enableUndo": false,
  "editor.guides.indentation": true,
  "javascript.suggest.completeFunctionCalls": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.spec.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock",
    "*.component.ts": "${capture}.component.html, ${capture}.component.scss, ${capture}.component.stories.ts",
    "*.module.ts": "${capture}.routing.ts",
    "*.directive.ts": "${capture}.directive.spec.ts, ${capture}.directive.stories.ts"
  },
  "explorer.fileNesting.expand": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.suggest.preview": true,
  "testExplorer.useNativeTesting": true,
  "testExplorer.addToEditorContextMenu": true,
  "window.commandCenter": true,
  "git.mergeEditor": true,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "vscode-edge-devtools.webhint": false,
  "editor.dragAndDrop": false,
  "editor.guides.bracketPairs": "active",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.snippetSuggestions": "bottom",
  "workbench.list.smoothScrolling": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.highlightModifiedTabs": true,
  "explorer.incrementalNaming": "smart",
  "angular.forceStrictTemplates": true,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "yarn",
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "editor.inlayHints.fontSize": 10,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "material-icon-theme.folders.theme": "classic",
  "terraform.languageServer.enable": true,
  "debug.javascript.autoAttachFilter": "always",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "editor.inlineSuggest.enabled": true,
  "editor.codeLensFontSize": 10,
  "gitlens.codeLens.enabled": false,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "workbench.colorTheme": "Dracula",
  "[scss]": {
    "editor.suggest.insertMode": "replace",
    "cSpell.fixSpellingWithRenameProvider": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "githubPullRequests.pullBranch": "never"
}
